<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.omret.foodsearch" >

    <uses-permission android:name="ANDROID.PERMISSION.INTERNET"></uses-permission>
    <uses-permission android:name="ANDROID.PERMISSION.ACCESS_NETWORK_STATE"></uses-permission>
    <uses-permission android:name="ANDROID.PERMISSION.CHANGE_NETWORK_STATE"></uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SigninActivity"
            android:label="@string/app_name"></activity>
        <activity android:name=".LogupActivity"
            android:label="@string/app_name"></activity>
        <activity android:name=".ShopDetailActivity"></activity>
        <activity android:name=".UserStarShopActivity"></activity>
        <activity android:name=".AllOrderActivity"></activity>
        <activity android:name=".CommitActivity"></activity>
    </application>

</manifest>
